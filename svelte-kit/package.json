{
  "name": "svelte-kit",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjschie/repo_name.git"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "npm-run-all --parallel --aggregate-output --continue-on-error 'lint:*'",
    "lint:format": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:ts": "tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "importSort": {
    ".js, .jsx, .es, .cjs, .mjs, .svelte": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.12",
    "postcss": "^8.4.4",
    "postcss-load-config": "^3.1.0",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "14.* || >=16"
  },
  "packageManager": "yarn@3.1.1"
}
