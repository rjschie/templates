{
  "name": "expencier",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjschie/expencier.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "setup": "yarn node scripts/setup.js"
  },
  "importSort": {
    ".js, .jsx, .es, .cjs, .svelte": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
  "engines": {
    "node": "14.* || >= 15"
  },
  "dependencies": {
    "eslint": "^8.0.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.2",
    "import-sort-style-eslint": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-svelte": "^2.4.0"
  },
  "packageManager": "yarn@3.1.0"
}
